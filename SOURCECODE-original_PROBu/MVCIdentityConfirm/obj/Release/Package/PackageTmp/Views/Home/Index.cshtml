    function rate(company) {
        var percent;
        $("#ocenaWord").text("");
        $.ajax({
            type: "GET",
            url: '@Url.Action("getCompRating" , "Rating")',
            dataType: "json",
            cache: false,
            data: { 'getData': company },
            success: function (dta) {
                $("#liczbaOsob").text(dta[1]);
                percent = dta[0] / 700;
                
                ocenaProcent = (percent * 100).toFixed(3);
                $("#procentOceny").text(ocenaProcent + " %");
                if (percent == 0) {
                    $("#ocenaWord").text("BRAK OCENY");
                }
                if (percent < 0) {
                    $("#ocenaWord").text("UWAGA! OBCHODZIĆ Z DALEKA!!! ");
                }
                if (percent > 0) {
                    $("#ocenaWord").text("FATALNE");
                }
                if (percent >= 0.2 && percent <= 0.4) {
                    $("#ocenaWord").text("KIEPSKIE");
                }
                if (percent > 0.4 && percent <= 0.6) {
                    $("#ocenaWord").text("PRZYZWOITE");
                }
                if (percent > 0.6 && percent <= 0.8) {
                    $("#ocenaWord").text("BARDZO DOBRE");
                }
                if (percent > 0.8) {
                    $("#ocenaWord").text("DOSKONAŁE !!!");
                }
                $('#progressbar').gradientProgressBar({
                    value: percent,                                                 //poprawić zarobki
                    size: 300,
                    fill: {
                        gradient: ["red", "yellow", "green"]
                    }
                });
            },
            error: function (xhr, ajaxOptions, thrownError) {
                alert(xhr.responseText);
            }
        });
    };


    function rateManager() {
        var idManag = $(".casaOcen").attr("id");
        $("#managerOcena").text("");
        $.ajax({
            type: "GET",
            cache: false,
            url: '@Url.Action("getMenRat" , "Rating")',
            dataType: "json",
            data: { 'getD': idManag },
            success: function (data1) {

                var wyn = 0;
                wyn = (data1[0] / 800);

                $("#liczbaOsobMan").text(data1[1]);
                if (wyn == 0 && data1[1] != 0) {
                    $("#managerOcena").text("BEZ KOMENTARZA");
                }
                if (wyn == 0 && data1[1] == 0) {
                    $("#managerOcena").text("BRAK OCENY");
                }
                if (wyn < 0 ) {
                    $("#managerOcena").text("ZWOLNIĆ NATYCHMIAST!");
                }
                if (wyn > 0 && wyn  <= 0.2 && wyn != 0) {
                    $("#managerOcena").text("TOTALNA ŻENADA!");
                }
                if (wyn > 0.2 && wyn <= 0.4) {
                    $("#managerOcena").text("KIEPSKO - POPRAW SIĘ!");
                }
                if (wyn > 0.4 && wyn <= 0.6) {
                    $("#managerOcena").text("NIEŹLE - MOŻE BYĆ LEPIEJ!");
                }
                if (wyn > 0.6 && wyn <= 0.8) {
                    $("#managerOcena").text("JEST DOBRZE ;)");
                }
                if (wyn > 0.8) {
                    $("#managerOcena").text("SUPER - TAK TRZYMAJ!");
                }

                $("#percentManagerRating").text((wyn * 100).toFixed(3) + " %");
                $('#prog').gradientProgressBar({
                    value: wyn,
                    size: 235,
                    fill: {
                        gradient: ["red", "yellow", "green"]
                    }
                });
            },
            error: function (xhr, ajaxOptions, thrownError) {
                alert(xhr.responseText);
            }
        });
    };

